        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Change menu option.

// Change options.
    if(initialized = true)
    {
        position = lerp(position, 0, 0.1);
        if(position &gt; -4)
        {
            main_menu_active = 1;
        }
    }
    if(main_menu_active == 1)
    {
       if(keyboard_check_pressed(global.ip_down))
       {
          if(main_menu_option &lt; 3)
          {
             main_menu_option += 1;
             audio_play(_general_menu_bleep, global.sfx_volume);   
          }
          else
          {
             main_menu_option = 0;
             audio_play(_general_menu_bleep, global.sfx_volume);
          }
       }
       if(keyboard_check_pressed(global.ip_up))
       {
          if(main_menu_option &gt; 0)
          {
             main_menu_option -= 1;
             audio_play(_general_menu_bleep, global.sfx_volume);     
          }
          else
          {
             main_menu_option = 3;
             audio_play(_general_menu_bleep, global.sfx_volume);  
          }
       }  
        // Choose.   
        if(keyboard_check_pressed(global.ip_button_s))
        {
          audio_play(_general_menu_accept, global.sfx_volume);
          switch(main_menu_option)
          {
                 case 0:
                 {
                      instance_create(0, 0, obj_video);
                      instance_destroy();
                      break;
                 }
                 case 1:
                 {
                      instance_create(0, 0, obj_input);
                      instance_destroy();
                      break;
                 }
                 case 2:
                 {
                      instance_create(0, 0, obj_sound);
                      instance_destroy();
                      break;
                 }
                 case 3:
                 {
                      instance_create(0, 0, obj_gameplay);
                      instance_destroy();
                      break;
                 }
          }
       }  
       
       // Leave.
       if(keyboard_check_pressed(global.ip_button_b))
       {
            audio_stop(msc_options);
            audio_loop(msc_menu, global.bgm_volume);
            instance_create(0, 0, obj_main_menu);
            instance_destroy();
       }   
    }
</string>
          </argument>
        </arguments>
      </action>
