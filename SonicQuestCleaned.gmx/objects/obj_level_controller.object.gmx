            <string>/// (Un)Pause Game.

    
    if(global.do_time == true &amp;&amp; instance_exists(par_fades) == false &amp;&amp; !instance_exists(obj_game_over)){
    
       // Check if we pressed Pause:
          if(keyboard_check_pressed(global.key_enter) &amp;&amp; !instance_exists(obj_title_card)){
           
             // Pause the Game:   
                if(game_paused == false &amp;&amp; game_snapshot == noone &amp;&amp; instance_exists(obj_pause) == false){
                   game_paused = true;
                   game_snapshot    = background_create_from_surface(application_surface, 0, 0, global.screen_width, global.screen_height, 0, 0);
                   instance_deactivate_all(1);
                   audio_pause_all();                   
                   if(global.BonusStage == false){
                      instance_create(0, 0, obj_pause);
                      instance_activate_object(obj_pause)
                   }
                   instance_activate_object(obj_gamepad_controller);
                   instance_activate_object(obj_screen_controller);
                   instance_activate_object(obj_audio);                
                   exit;
                }
                
             // Unpause the Game:
                if(game_paused == true &amp;&amp; game_snapshot != noone){            
                   background_delete(game_snapshot);
                   game_paused = false;
                   game_snapshot    = noone;
                   instance_activate_all();
                   audio_resume_all();  
                   if(instance_exists(obj_pause)){
                      with(obj_pause){
                           Finished = true;
                      }
                   }
                   exit;                 
                }
             
          }
    
    } 


</string>
