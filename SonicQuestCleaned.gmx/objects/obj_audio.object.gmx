                    zone_bgm = boss_bgm;
                    aud_play_music(boss_bgm, global.bgm_volume, 1);  
               }
      }
      else
      {
            if(audio_is_playing(boss_bgm) &amp;&amp; zone_bgm != -1)
               {
                  //show_message("GOODBYE SOUND")
                    audio_stop_sound(boss_bgm);
                    zone_bgm = temp_zone_bgm;
                    aud_play_music(zone_bgm, global.bgm_volume, 1);  
               }
      } 
 
    
   // Final Boss Theme
      if (final_boss_flag)
      {
          if(audio_is_playing(zone_bgm) &amp;&amp; !audio_is_playing(final_boss_bgm) &amp;&amp; zone_bgm != -1)
