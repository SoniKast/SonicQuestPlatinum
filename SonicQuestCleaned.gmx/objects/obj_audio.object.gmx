    else
    {
       if(audio_is_playing(zone_bgm) &amp;&amp; zone_bgm != -1)
       {    
          audio_sound_gain(zone_bgm, 0.00, 0);   
       }  
    }
    if(audio_is_playing(jingle_drown) == false)
    {    
       if(audio_is_playing(jingle_sneakers))
